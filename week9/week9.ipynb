   "metadata": {},
   "outputs": [],
   "source": [
    "fibTable = {}\n",
    "\n",
    "def fib(n) : \n",
    "    if n in fibTable.keys() : \n",
    "        return fibTable[n]\n",
    "    \n",
    "    if n <= 1 : \n",
    "        value = n \n",
    "    else : \n",
    "        value = fib(n-1) + fib(n-2)\n",
    "\n",
    "    fibTable[n] = value \n",
    "    return value "
   ]
